# Default environment variables template
# This will be used as a base for all client configurations

# Client Information
CLIENT_ID=default
CLIENT_NAME="Default Client"

# Shared MCP URLs (to be updated with actual Railway URLs)
GOOGLE_DRIVE_MCP_URL=https://shared-google-drive-mcp.railway.app/sse
SLACK_MCP_URL=https://shared-slack-mcp.railway.app/sse

# OAuth credentials (to be filled in per client)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
SLACK_CLIENT_ID=your_slack_client_id
SLACK_CLIENT_SECRET=your_slack_client_secret

# Server Configuration
PORT=3080
NODE_ENV=production 